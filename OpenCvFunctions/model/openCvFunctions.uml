      <body>using namespace cv;

std::cout&lt;&lt;&quot;Bild bearbeiten...&quot; &lt;&lt; std::endl;

std::shared_ptr&lt;cv::Mat>  dst = std::make_shared&lt;cv::Mat>();
int top, bottom, left, right;
int borderType = BORDER_CONSTANT;
const char* window_name = &quot;copyMakeBorder Demo&quot;;
RNG rng(12345);

  if( inputImage->empty() )
    {
      printf(&quot; No data entered, please enter the path to an image file \n&quot;);
    }

  namedWindow( window_name, WINDOW_AUTOSIZE );
  top = (int) (0.05*(inputImage->rows)); bottom = (int) (0.05*(inputImage->rows));
  left = (int) (0.05*(inputImage->cols)); right = (int) (0.05*(inputImage->cols));
  *dst = *inputImage;
  imshow( window_name, *dst );
  Scalar value( rng.uniform(0, 255), rng.uniform(0, 255), rng.uniform(0, 255) );
  copyMakeBorder( *inputImage, *dst, top, bottom, left, right, borderType, value );
  imshow( window_name, *dst );

imwrite( &quot;lena1.png&quot;, * dst);</body>
